{
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "apps/api-secret-commands/src",
    "projectType": "application",
    "targets": {
        "build": {
            "executor": "@nrwl/node:webpack",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/apps/api-secret-commands",
                "main": "apps/api-secret-commands/src/main.ts",
                "tsConfig": "apps/api-secret-commands/tsconfig.app.json"
            },
            "configurations": {
                "production": {
                    "optimization": true,
                    "extractLicenses": true,
                    "inspect": false
                }
            }
        },
        "serve": {
            "executor": "@nrwl/node:node",
            "options": {
                "buildTarget": "api-secret-commands:build"
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["apps/api-secret-commands/**/*.ts"]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["coverage/apps/api-secret-commands"],
            "options": {
                "jestConfig": "apps/api-secret-commands/jest.config.ts",
                "passWithNoTests": true
            }
        }
    },
    "tags": []
}
